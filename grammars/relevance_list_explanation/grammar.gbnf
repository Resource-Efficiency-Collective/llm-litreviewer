root ::= AbstractInfo
AbstractInfo ::= "{" ws "\"relevant\":" ws boolean "," ws "\"mitigationmeasures\":" ws stringlist "," ws "\"explanation\":" ws string "}"
AbstractInfolist ::= "[]" | "[" ws AbstractInfo ("," ws AbstractInfo)* ws "]"

string ::= "\"" ([^"]*) "\""
boolean ::= "0" | "1"
ws ::= [ \t\n]*
number ::= [0-9]+ "."? [0-9]*
stringlist ::= "[" ws "]" | "[" ws string ("," ws string)* ws "]"
numberlist ::= "[" ws "]" | "[" ws number ("," ws number)* ws "]"
