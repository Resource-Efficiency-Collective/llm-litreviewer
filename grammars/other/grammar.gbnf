root ::= AbstractInfo
AbstractInfo ::= "{"   ws   "\"relevant\":"   ws   boolean   ","   ws   "\"mitigationmeasures\":"   ws   stringlist   "}"
AbstractInfolist ::= "[]" | "["   ws   AbstractInfo   (","   ws   AbstractInfo)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
